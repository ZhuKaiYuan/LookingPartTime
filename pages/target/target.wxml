
<view class='targetView' wx:for="{{targetList}}" wx:key="index"> 
  <i-card title="{{item.name}}" extra="{{item.position}}" thumb="{{item.imgUrl}}">
    <view slot="content">{{item.title}}</view>
    <view slot="footer">{{item.content}}</view>
  </i-card>
  <button class='btn' bindtap='itemDidClick'>跳转</button>
</view>


  


