.i-card{
  font-size:16rpx;
  overflow:hidden;
  background:#fff;
  border-radius:10rpx
  }
.i-card-header{
  display:flex;
  justify-content:flex-start;
  align-items:center;
  padding:1rpx 10rpx;
  min-height:200rpx;
}
.mianImage{
  width:140rpx;
  height:140rpx;
  margin:0 30rpx 0 15rpx;
}
.imgText{
height:140rpx;
width:500rpx;
}
.title{
  height:30rpx;
  line-height:30rpxpx;
  font-size:22rpx;
  margin-bottom:15rpx;
  color:#404040;
  padding-right:28rpx;
  display:flex;
  justify-content:space-between;
  align-items:center;
  }
.textspan{
  font-size:20rpx;
  }
.titleps{
  line-height:25rpx;
  font-size:18rpx;
  color:#aaa;
  padding-right:28rpx;
  }
.timerTile{
  height:50rpx;
  line-height:50rpx;
  border-top:1px solid #eee;
  padding-left:20rpx;
  font-size:20rpx;
  color:#555;
}